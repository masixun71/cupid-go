CGO_ENABLED=0 GOOS=windows GOARCH=amd64 go build -ldflags '-w -s' cupid.go && upx cupid.exe && zip cupid_win64.zip cupid.exe && CGO_ENABLED=0 GOOS=linux GOARCH=amd64 go build -ldflags '-w -s' cupid.go && upx cupid && tar cvf cupid_amd64_linux.tar cupid && rm -rf cupid_amd64_linux.tar.xz && xz -z cupid_amd64_linux.tar && go build -ldflags '-w -s' cupid.go && upx cupid && tar cvf cupid_darwin64_mac.tar cupid && rm cupid_darwin64_mac.tar.xz && xz -z cupid_darwin64_mac.tar